<!DOCTYPE html>
<html>
<head>
  <title>Swiss</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if defined?(@user.errors) %>
  <% @user.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
<% end %>

<%= yield %>

</body>
</html>
