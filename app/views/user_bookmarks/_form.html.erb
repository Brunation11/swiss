<%=form_for @user_bookmark do |f|%>
  <%=f.label "Name you would like to save this bookmark as"%>
  <%=f.text_field :name, required: true%>
  <%=f.fields_for :bookmark do |g|%>
    <%=g.label "URL of site you would like to bookmark"%>
    <%=g.text_field "url", required: true, disabled: !@user_bookmark.new_record?%>
  <%=f.submit%>
  <%end%>
<%end%>

