<%=form_for @user_bookmark do |f|%>
  <%=f.text_field :name, required: true, placeholder: "Name", id: "url-name-form-field", class: "form-field" %>
  <%=f.fields_for :bookmark do |g|%>
    <%=g.text_field "url", required: true, disabled: !@user_bookmark.new_record?, placeholder: "URL", id: "url-form-field", class: "form-field" %>
  <%= submit_tag %>
  <%end%>
<%end%>

