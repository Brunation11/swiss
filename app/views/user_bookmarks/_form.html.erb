<%=form_for [@user, @user_bookmark] do |f|%>
  <%=f.label "URL of site you would like to bookmark"%>
  <div>
    <input type="text" name='URL' />
  </div>
  <%=f.label "Name you would like to save this bookmark as"%>
  <%=f.text_field :name%>
  <%=f.label "Categories you would like to tag this Bookmark with (separated by commas)"%>
  <div>
    <input type="text" name='tags' />
  </div>
  <%=f.submit%>
<%end%>